\begin_body

\begin_layout Standard
Generally
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E\left(X\right)=\underset{X}{\min}\left\Vert b-f\left(X\right)\right\Vert ^{2}\implies f\left(X\right)^{T}f\left(X\right)-2b^{T}f\left(X\right)
\]

\end_inset


\end_layout

\begin_layout Standard
wish to minimize the error so take the partial with respect to the state
 and find where it is 0
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial E\left(X\right)}{\partial X}=2\frac{\partial f\left(X\right)}{\partial X}^{T}f\left(X\right)-2\frac{\partial f\left(X\right)}{\partial X}^{T}b=0
\]

\end_inset


\end_layout

\begin_layout Standard
approximating 
\begin_inset Formula $f\left(X\right)$
\end_inset


\begin_inset Formula 
\[
f\left(X+\Delta X\right)=f\left(X\right)+\frac{\partial f\left(X\right)}{\partial X}\Delta X
\]

\end_inset

substituting in
\begin_inset Formula 
\[
\frac{\partial f\left(X\right)}{\partial X}^{T}\left(f\left(X\right)+\frac{\partial f\left(X\right)}{\partial X}\Delta X\right)-\frac{\partial f\left(X\right)}{\partial X}^{T}b=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial f\left(X\right)}{\partial X}^{T}f\left(X\right)+\frac{\partial f\left(X\right)}{\partial X}^{T}\frac{\partial f\left(X\right)}{\partial X}\Delta X-\frac{\partial f\left(X\right)}{\partial X}^{T}b=0
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial f\left(X\right)}{\partial X}^{T}\frac{\partial f\left(X\right)}{\partial X}\Delta X=\frac{\partial f\left(X\right)}{\partial X}^{T}\left(b-f\left(X\right)\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
X_{k+1}=X_{k}+\Delta X
\]

\end_inset


\end_layout

\begin_layout Standard
Take the initial features and perform bundle adjustment to determine the
 location in each new frame
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\underline{p}_{i/c}\left(t\right)=\underline{p}_{k/c}\left(t\right)+R_{k/c}\left(t\right)\underline{p}_{i/k}
\]

\end_inset


\begin_inset Formula 
\[
R_{k/c}\left(t\right)=\begin{bmatrix}r_{11} & r_{12} & r_{13}\\
r_{21} & r_{22} & r_{23}\\
r_{31} & r_{32} & r_{33}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
q_{k/c}\left(t\right)=\begin{bmatrix}q_{w}\left(t\right)\\
0\\
q_{y}\left(t\right)\\
0
\end{bmatrix}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
q_{w}=\cos\left(\frac{\theta}{2}\right)
\]

\end_inset


\begin_inset Formula 
\[
q_{y}=\sin\left(\frac{\theta}{2}\right)
\]

\end_inset


\begin_inset Formula 
\[
\underline{p}_{k/c}\left(t\right)=\begin{bmatrix}x_{k/c}\left(t\right)\\
0\\
z_{k/c}\left(t\right)
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
r_{11}=1-2q_{y}^{2}
\]

\end_inset


\begin_inset Formula 
\[
r_{12}=0
\]

\end_inset


\begin_inset Formula 
\[
r_{13}=2q_{y}q_{w}
\]

\end_inset


\begin_inset Formula 
\[
r_{21}=0
\]

\end_inset


\begin_inset Formula 
\[
r_{22}=1
\]

\end_inset


\begin_inset Formula 
\[
r_{23}=0
\]

\end_inset


\begin_inset Formula 
\[
r_{31}=-2q_{y}q_{w}
\]

\end_inset


\begin_inset Formula 
\[
r_{32}=0
\]

\end_inset


\begin_inset Formula 
\[
r_{33}=1-2q_{y}^{2}
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}x_{i/c}\left(t\right)\\
y_{i/c}\left(t\right)\\
z_{i/c}\left(t\right)
\end{bmatrix}=\begin{bmatrix}x_{k/c}\left(t\right)\\
y_{k/c}\\
z_{k/c}\left(t\right)
\end{bmatrix}+\begin{bmatrix}r_{11} & 0 & r_{13}\\
0 & 1 & 0\\
r_{31} & 0 & r_{33}
\end{bmatrix}\begin{bmatrix}x_{i/k}\\
y_{i/k}\\
z_{i/k}
\end{bmatrix}
\]

\end_inset

dropping the time for notational briefness
\begin_inset Formula 
\[
\begin{bmatrix}x_{i/c}\\
y_{i/c}\\
z_{i/c}
\end{bmatrix}=\begin{bmatrix}x_{k/c}+r_{11}x_{i/k}+r_{13}z_{i/k}\\
y_{k/c}+y_{i/k}\\
z_{k/c}+r_{31}x_{i/k}+r_{33}z_{i/k}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}x_{i/c}\\
y_{i/c}\\
z_{i/c}
\end{bmatrix}=\begin{bmatrix}x_{k/c}+\left(1-2q_{y}^{2}\right)x_{i/k}+2q_{y}q_{w}z_{i/k}\\
y_{k/c}+y_{i/k}\\
z_{k/c}-2q_{y}q_{w}x_{i/k}+\left(1-2q_{y}^{2}\right)z_{i/k}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}x_{i/c}\\
y_{i/c}\\
z_{i/c}
\end{bmatrix}=\begin{bmatrix}x_{k/c}+x_{i/k}-2q_{y}^{2}x_{i/k}+2q_{y}q_{w}z_{i/k}\\
y_{k/c}+y_{i/k}\\
z_{k/c}-2q_{y}q_{w}x_{i/k}+z_{i/k}-2q_{y}^{2}z_{i/k}
\end{bmatrix}
\]

\end_inset


\end_layout

\begin_layout Standard
projective geometry
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{bmatrix}u_{i/c}\\
v_{i/c}
\end{bmatrix}=\begin{bmatrix}f_{x}m_{x}+c_{x}\\
f_{y}m_{y}+c_{y}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}m_{x}\\
m_{y}
\end{bmatrix}=\begin{bmatrix}\frac{x_{i/c}}{z_{i/c}}\\
\frac{y_{i/c}}{z_{i/c}}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}u_{i/c}\\
v_{i/c}
\end{bmatrix}=\begin{bmatrix}f_{x}\frac{x_{i/c}}{z_{i/c}}+c_{x}\\
f_{y}\frac{y_{i/c}}{z_{i/c}}+c_{y}
\end{bmatrix}
\]

\end_inset


\end_layout

\begin_layout Standard
state to estimate
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
X=\begin{bmatrix}q_{w}\\
q_{y}\\
x_{k/c}\\
z_{k/c}\\
x_{i/k}\\
y_{i/k}\\
z_{i/k}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
J_{i}=\begin{bmatrix}\frac{\partial u_{i/c}}{\partial q_{w}} & \frac{\partial u_{i/c}}{\partial q_{y}} & \frac{\partial u_{i/c}}{\partial x_{k/c}} & \frac{\partial u_{i/c}}{\partial z_{k/c}} & \frac{\partial u_{i/c}}{\partial x_{i/k}} & \frac{\partial u_{i/c}}{\partial y_{i/k}} & \frac{\partial u_{i/c}}{\partial z_{i/k}}\\
\frac{\partial v_{i/c}}{\partial q_{w}} & \frac{\partial v_{i/c}}{\partial q_{y}} & \frac{\partial v_{i/c}}{\partial x_{k/c}} & \frac{\partial v_{i/c}}{\partial z_{k/c}} & \frac{\partial v_{i/c}}{\partial x_{i/k}} & \frac{\partial v_{i/c}}{\partial y_{i/k}} & \frac{\partial v_{i/c}}{\partial z_{i/k}}
\end{bmatrix}
\]

\end_inset


\end_layout

\begin_layout Standard
partials for rotation elements
\end_layout

\begin_layout Standard
partials of u and v, using 
\begin_inset Formula $x_{i/c},y_{i/c},z_{i/c}$
\end_inset

 where appropriate for notationaly briefness
\begin_inset Formula 
\[
\frac{\partial u_{i/c}}{\partial X}=f_{x}\frac{z_{i/c}}{z_{i/c}^{2}}\frac{\partial x_{i/c}}{\partial X}-f_{x}\frac{x_{i/c}}{z_{i/c}^{2}}\frac{\partial z_{i/c}}{\partial X}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial v_{i/c}}{\partial X_{i}}=f_{y}\frac{z_{i/c}}{z_{i/c}^{2}}\frac{\partial y_{i/c}}{\partial X}-f_{y}\frac{y_{i/c}}{z_{i/c}^{2}}\frac{\partial z_{i/c}}{\partial X}
\]

\end_inset

partials 
\begin_inset Formula $\frac{\partial x_{i/c}}{\partial X}$
\end_inset


\begin_inset Formula 
\[
x_{i/c}=x_{k/c}+x_{i/k}-2q_{y}^{2}x_{i/k}+2q_{y}q_{w}z_{i/k}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial x_{i/c}}{\partial q_{w}}=2q_{y}z_{i/k}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial x_{i/c}}{\partial q_{y}}=-4q_{y}x_{i/k}+2q_{w}z_{i/k}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial x_{i/c}}{\partial x_{k/c}}=1
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial x_{i/c}}{\partial z_{k/c}}=0
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial x_{i/c}}{\partial x_{i/k}}=1-2q_{y}^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial x_{i/c}}{\partial y_{i/k}}=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial x_{i/c}}{\partial z_{i/k}}=2q_{y}q_{w}
\]

\end_inset


\end_layout

\begin_layout Standard
partials 
\begin_inset Formula $\frac{\partial y_{i/c}}{\partial X}$
\end_inset


\begin_inset Formula 
\[
y_{i/c}=y_{k/c}+y_{i/k}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial y_{i/c}}{\partial q_{w}}=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial y_{i/c}}{\partial q_{y}}=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial y_{i/c}}{\partial x_{k/c}}=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial y_{i/c}}{\partial z_{k/c}}=0
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial y_{i/c}}{\partial x_{i/k}}=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial y_{i/c}}{\partial y_{i/k}}=1
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial y_{i/c}}{\partial z_{i/k}}=0
\]

\end_inset


\end_layout

\begin_layout Standard
partials for 
\begin_inset Formula $\frac{\partial z_{i/c}}{\partial X}$
\end_inset


\begin_inset Formula 
\[
z_{i/c}=z_{k/c}-2q_{y}q_{w}x_{i/k}+z_{i/k}-2q_{y}^{2}z_{i/k}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial z_{i/c}}{\partial q_{w}}=-2q_{y}x_{i/k}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial z_{i/c}}{\partial q_{y}}=-2q_{w}x_{i/k}-4q_{y}z_{i/k}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial z_{i/c}}{\partial x_{k/c}}=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial z_{i/c}}{\partial z_{k/c}}=1
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial z_{i/c}}{\partial x_{i/k}}=-2q_{y}q_{w}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial z_{i/c}}{\partial y_{i/k}}=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial z_{i/c}}{\partial z_{i/k}}=1-2q_{y}^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
stacking the partials across all features
\begin_inset Formula 
\[
\frac{\partial u_{i/c}}{\partial X}=f_{x}\frac{z_{i/c}}{z_{i/c}^{2}}\frac{\partial x_{i/c}}{\partial X}-f_{x}\frac{x_{i/c}}{z_{i/c}^{2}}\frac{\partial z_{i/c}}{\partial X}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial v_{i/c}}{\partial X}=f_{y}\frac{z_{i/c}}{z_{i/c}^{2}}\frac{\partial y_{i/c}}{\partial X}-f_{y}\frac{y_{i/c}}{z_{i/c}^{2}}\frac{\partial z_{i/c}}{\partial X}
\]

\end_inset

 
\begin_inset Formula 
\[
X^{n}=\begin{bmatrix}q_{w}^{n}\\
q_{y}^{n}\\
x_{k/c}^{n}\\
z_{k/c}^{n}\\
x_{i/k}\\
y_{i/k}\\
z_{i/k}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\eta^{n}=\begin{bmatrix}q_{w}^{n}\\
q_{y}^{n}\\
x_{k/c}^{n}\\
z_{k/c}^{n}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\underline{p}_{i/k}=\begin{bmatrix}x_{i/k}\\
y_{i/k}\\
z_{i/k}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
J^{n}=\begin{bmatrix}\frac{\partial u_{1/c}}{\partial q_{w}}^{n} & \frac{\partial u_{1/c}}{\partial q_{y}}^{n} & \frac{\partial u_{1/c}}{\partial x_{k/c}}^{n} & \frac{\partial u_{1/c}}{\partial z_{k/c}}^{n} & \frac{\partial u_{1/c}}{\partial x_{1/k}}^{n} & \frac{\partial u_{1/c}}{\partial y_{1/k}}^{n} & \frac{\partial u_{1/c}}{\partial z_{1/k}}^{n} & \dots & 0 & 0 & 0\\
\frac{\partial v_{i/c}}{\partial q_{w}}^{n} & \frac{\partial v_{i/c}}{\partial q_{y}}^{n} & \frac{\partial v_{i/c}}{\partial x_{k/c}}^{n} & \frac{\partial v_{i/c}}{\partial z_{k/c}}^{n} & \frac{\partial v_{1/c}}{\partial x_{1/k}}^{n} & \frac{\partial v_{1/c}}{\partial y_{1/k}}^{n} & \frac{\partial v_{1/c}}{\partial z_{1/k}}^{n} & \dots & 0 & 0 & 0\\
\vdots & \vdots & \vdots & \vdots & \ddots & \ddots & \ddots & \ddots & \vdots & \vdots & \vdots\\
\frac{\partial u_{p/c}}{\partial q_{w}}^{n} & \frac{\partial u_{p/c}}{\partial q_{y}}^{n} & \frac{\partial u_{p/c}}{\partial x_{k/c}}^{n} & \frac{\partial u_{p/c}}{\partial z_{k/c}}^{n} & 0 & 0 & 0 & \dots & \frac{\partial u_{p/c}}{\partial x_{p/k}}^{n} & \frac{\partial u_{p/c}}{\partial y_{p/k}}^{n} & \frac{\partial u_{p/c}}{\partial z_{p/k}}^{n}\\
\frac{\partial v_{p/c}}{\partial q_{w}}^{n} & \frac{\partial v_{i/c}}{\partial q_{y}}^{n} & \frac{\partial v_{p/c}}{\partial x_{k/c}}^{n} & \frac{\partial v_{p/c}}{\partial z_{k/c}}^{n} & 0 & 0 & 0 & \dots & \frac{\partial v_{p/c}}{\partial x_{p/k}}^{n} & \frac{\partial v_{p/c}}{\partial y_{p/k}}^{n} & \frac{\partial v_{p/c}}{\partial z_{p/k}}^{n}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
J^{n}\in\mathbb{R}^{2p\times4+3p}
\]

\end_inset


\begin_inset Formula 
\[
J_{\eta}^{n}=\begin{bmatrix}\frac{\partial u_{1/c}}{\partial q_{w}}^{n} & \frac{\partial u_{1/c}}{\partial q_{y}}^{n} & \frac{\partial u_{1/c}}{\partial x_{k/c}}^{n} & \frac{\partial u_{1/c}}{\partial z_{k/c}}^{n}\\
\frac{\partial v_{i/c}}{\partial q_{w}}^{n} & \frac{\partial v_{i/c}}{\partial q_{y}}^{n} & \frac{\partial v_{i/c}}{\partial x_{k/c}}^{n} & \frac{\partial v_{i/c}}{\partial z_{k/c}}^{n}\\
\vdots & \vdots & \vdots & \vdots\\
\frac{\partial u_{p/c}}{\partial q_{w}}^{n} & \frac{\partial u_{p/c}}{\partial q_{y}}^{n} & \frac{\partial u_{p/c}}{\partial x_{k/c}}^{n} & \frac{\partial u_{p/c}}{\partial z_{k/c}}^{n}\\
\frac{\partial v_{p/c}}{\partial q_{w}}^{n} & \frac{\partial v_{i/c}}{\partial q_{y}}^{n} & \frac{\partial v_{p/c}}{\partial x_{k/c}}^{n} & \frac{\partial v_{p/c}}{\partial z_{k/c}}^{n}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
J_{\eta}^{n}\in\mathbb{R}^{2p\times4}
\]

\end_inset


\begin_inset Formula 
\[
J_{p}^{n}=\begin{bmatrix}\frac{\partial u_{1/c}}{\partial x_{1/k}}^{n} & \frac{\partial u_{1/c}}{\partial y_{1/k}}^{n} & \frac{\partial u_{1/c}}{\partial z_{1/k}}^{n} & \dots & 0 & 0 & 0\\
\frac{\partial v_{1/c}}{\partial x_{1/k}}^{n} & \frac{\partial v_{1/c}}{\partial y_{1/k}}^{n} & \frac{\partial v_{1/c}}{\partial z_{1/k}}^{n} & \dots & 0 & 0 & 0\\
\ddots & \ddots & \ddots & \ddots & \vdots & \vdots & \vdots\\
0 & 0 & 0 & \dots & \frac{\partial u_{p/c}}{\partial x_{p/k}}^{n} & \frac{\partial u_{p/c}}{\partial y_{p/k}}^{n} & \frac{\partial u_{p/c}}{\partial z_{p/k}}^{n}\\
0 & 0 & 0 & \dots & \frac{\partial v_{p/c}}{\partial x_{p/k}}^{n} & \frac{\partial v_{p/c}}{\partial y_{p/k}}^{n} & \frac{\partial v_{p/c}}{\partial z_{p/k}}^{n}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
J_{p}^{n}\in\mathbb{R}^{2p\times3p}
\]

\end_inset


\begin_inset Formula 
\[
X=\begin{bmatrix}\eta^{1}\\
\vdots\\
\eta^{N}\\
\underline{p}_{1/k}\\
\vdots\\
\underline{p}_{p/k}
\end{bmatrix}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
X\in\mathbb{R}^{4N+3p}
\]

\end_inset


\begin_inset Formula 
\[
2p\times4+3p
\]

\end_inset


\begin_inset Formula 
\[
J=\begin{bmatrix}J_{\eta}^{1} & 0 & 0 & J_{p}^{1}\\
0 & \ddots & 0 & \vdots\\
0 & 0 & J_{\eta}^{N} & J_{p}^{N}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
J\in\mathbb{R}^{2pN\times4N+3p}
\]

\end_inset

want to have
\begin_inset Formula 
\[
2pN>4N+3p
\]

\end_inset


\begin_inset Formula 
\[
2pN-4N>3p
\]

\end_inset


\begin_inset Formula 
\[
\left(2p-4\right)N>3p
\]

\end_inset


\begin_inset Formula 
\[
p\geq3
\]

\end_inset


\begin_inset Formula 
\[
N>\frac{3p}{\left(2p-4\right)}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
p=3
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
N>\frac{9}{2}\implies N\geq5
\]

\end_inset


\begin_inset Formula 
\[
p=20
\]

\end_inset


\begin_inset Formula 
\[
N>\frac{60}{33}
\]

\end_inset


\begin_inset Formula 
\[
N>\frac{60}{\left(40-7\right)}
\]

\end_inset


\end_layout

\begin_layout Standard
if 
\begin_inset Formula $p=20$
\end_inset

 and 
\begin_inset Formula $N=5$
\end_inset


\begin_inset Formula 
\[
J\in\mathbb{R}^{200\times95}
\]

\end_inset


\end_layout

\begin_layout Standard
additional relationships 
\begin_inset Formula 
\[
q^{T}q=1
\]

\end_inset


\begin_inset Formula 
\[
q_{w}^{2}+q_{x}^{2}+q_{y}^{2}+q_{z}^{2}=1
\]

\end_inset


\begin_inset Formula 
\[
J=\begin{bmatrix}\frac{\partial}{\partial q_{w}} & \frac{\partial}{\partial q_{x}} & \frac{\partial}{\partial q_{y}} & \frac{\partial}{\partial q_{z}} & \frac{\partial}{\partial x_{k/c}} & \frac{\partial}{\partial y_{k/c}} & \frac{\partial}{\partial z_{k/c}} & \frac{\partial}{\partial x_{i/k}} & \frac{\partial}{\partial y_{i/k}} & \frac{\partial}{\partial z_{i/k}}\\
2q_{w} & 2q_{x} & 2q_{y} & 2q_{z} & 0 & 0 & 0 & 0 & 0 & 0
\end{bmatrix}
\]

\end_inset


