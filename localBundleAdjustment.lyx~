\frac{\partial u_{i/k}}{\partial z_{i/c}}=f_{x}\frac{z_{i/k}\left(r_{11}\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}+r_{21}\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}+r_{31}\right)-x_{i/k}\left(r_{13}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}\right)+r_{23}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}\right)+r_{33}\right)}{z_{i/k}^{2}}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial u_{i/k}}{\partial q_{w}}=f_{x}\frac{z_{i/k}\left(2q_{z}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)+2\left(q_{z}-q_{y}\right)\left(z_{i/c}-z_{k/c}\right)\right)-x_{i/k}\left(2q_{y}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)-2q_{x}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)\right)}{z_{i/k}^{2}}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial u_{i/k}}{\partial q_{x}}=f_{x}\frac{z_{i/k}\left(2q_{y}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)\right)-x_{i/k}\left(2q_{z}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)-2q_{w}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)-4q_{x}\left(z_{i/c}-z_{k/c}\right)\right)}{z_{i/k}^{2}}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial u_{i/k}}{\partial q_{y}}=f_{x}\frac{z_{i/k}\left(-4q_{y}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)+2q_{x}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)-2q_{w}\left(z_{i/c}-z_{k/c}\right)\right)-x_{i/k}\left(2q_{w}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)+2q_{z}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)-4q_{y}\left(z_{i/c}-z_{k/c}\right)\right)}{z_{i/k}^{2}}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial u_{i/k}}{\partial q_{z}}=f_{x}\frac{z_{i/k}\left(-4q_{z}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)+2q_{w}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)+2q_{w}\left(z_{i/c}-z_{k/c}\right)\right)-x_{i/k}\left(2q_{x}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)+2q_{y}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)\right)}{z_{i/k}^{2}}
