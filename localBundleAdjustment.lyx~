\begin_body

\begin_layout Standard
Generally
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E\left(X\right)=\underset{X}{\min}\left\Vert b-f\left(X\right)\right\Vert ^{2}\implies f\left(X\right)^{T}f\left(X\right)-2b^{T}f\left(X\right)
\]

\end_inset


\end_layout

\begin_layout Standard
wish to minimize the error so take the partial with respect to the state
 and find where it is 0
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial E\left(X\right)}{\partial X}=2\frac{\partial f\left(X\right)}{\partial X}^{T}f\left(X\right)-2\frac{\partial f\left(X\right)}{\partial X}^{T}b=0
\]

\end_inset


\end_layout

\begin_layout Standard
approximating 
\begin_inset Formula $f\left(X\right)$
\end_inset


\begin_inset Formula 
\[
f\left(X+\Delta X\right)=f\left(X\right)+\frac{\partial f\left(X\right)}{\partial X}\Delta X
\]

\end_inset

substituting in
\begin_inset Formula 
\[
\frac{\partial f\left(X\right)}{\partial X}^{T}\left(f\left(X\right)+\frac{\partial f\left(X\right)}{\partial X}\Delta X\right)-\frac{\partial f\left(X\right)}{\partial X}^{T}b=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial f\left(X\right)}{\partial X}^{T}f\left(X\right)+\frac{\partial f\left(X\right)}{\partial X}^{T}\frac{\partial f\left(X\right)}{\partial X}\Delta X-\frac{\partial f\left(X\right)}{\partial X}^{T}b=0
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial f\left(X\right)}{\partial X}^{T}\frac{\partial f\left(X\right)}{\partial X}\Delta X=\frac{\partial f\left(X\right)}{\partial X}^{T}\left(b-f\left(X\right)\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
X_{k+1}=X_{k}+\Delta X
\]

\end_inset


\end_layout

\begin_layout Standard
Take the initial features and perform bundle adjustment to determine the
 location in each new frame
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\underline{p}_{i/c}\left(t\right)=\underline{p}_{k/c}\left(t\right)+R_{k/c}\left(t\right)\underline{p}_{i/k}
\]

\end_inset


\begin_inset Formula 
\[
R_{k/c}\left(t\right)=\begin{bmatrix}r_{11} & r_{12} & r_{13}\\
r_{21} & r_{22} & r_{23}\\
r_{31} & r_{32} & r_{33}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
q_{k/c}\left(t\right)=\begin{bmatrix}q_{w}\left(t\right)\\
q_{x}\left(t\right)\\
q_{y}\left(t\right)\\
q_{z}\left(t\right)
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\underline{p}_{k/c}\left(t\right)=\begin{bmatrix}x_{k/c}\left(t\right)\\
y_{k/c}\left(t\right)\\
z_{k/c}\left(t\right)
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
r_{11}=1-2\left(q_{y}^{2}+q_{z}^{2}\right)
\]

\end_inset


\begin_inset Formula 
\[
r_{12}=2\left(q_{x}q_{y}-q_{z}q_{w}\right)
\]

\end_inset


\begin_inset Formula 
\[
r_{13}=2\left(q_{x}q_{z}+q_{y}q_{w}\right)
\]

\end_inset


\begin_inset Formula 
\[
r_{21}=2\left(q_{x}q_{y}+q_{z}q_{w}\right)
\]

\end_inset


\begin_inset Formula 
\[
r_{22}=1-2\left(q_{x}^{2}+q_{z}^{2}\right)
\]

\end_inset


\begin_inset Formula 
\[
r_{23}=2\left(q_{y}q_{z}-q_{x}q_{w}\right)
\]

\end_inset


\begin_inset Formula 
\[
r_{31}=2\left(q_{w}q_{z}-q_{y}q_{w}\right)
\]

\end_inset


\begin_inset Formula 
\[
r_{32}=2\left(q_{y}q_{z}+q_{x}q_{w}\right)
\]

\end_inset


\begin_inset Formula 
\[
r_{33}=1-2\left(q_{x}^{2}+q_{y}^{2}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
or
\begin_inset Formula 
\[
\underline{p}_{i/k}=R_{k/c}^{T}\left(t\right)\left(\underline{p}_{i/c}\left(t\right)-\underline{p}_{k/c}\left(t\right)\right)
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}x_{i/k}\\
y_{i/k}\\
z_{i/k}
\end{bmatrix}=R_{k/c}^{T}\left(t\right)\left(\underline{p}_{i/c}\left(t\right)-\underline{p}_{k/c}\left(t\right)\right)
\]

\end_inset


\end_layout

\begin_layout Standard
dropping the time for notational briefness
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{bmatrix}x_{i/k}\\
y_{i/k}\\
z_{i/k}
\end{bmatrix}=\begin{bmatrix}r_{11} & r_{12} & r_{13}\\
r_{21} & r_{22} & r_{23}\\
r_{31} & r_{32} & r_{33}
\end{bmatrix}^{T}\left(\begin{bmatrix}x_{i/c}\\
y_{i/c}\\
z_{i/c}
\end{bmatrix}-\begin{bmatrix}x_{k/c}\\
y_{k/c}\\
z_{k/c}
\end{bmatrix}\right)
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}x_{i/k}\\
y_{i/k}\\
z_{i/k}
\end{bmatrix}=\begin{bmatrix}r_{11} & r_{21} & r_{31}\\
r_{12} & r_{22} & r_{32}\\
r_{13} & r_{23} & r_{33}
\end{bmatrix}\left(\begin{bmatrix}x_{i/c}\\
y_{i/c}\\
z_{i/c}
\end{bmatrix}-\begin{bmatrix}x_{k/c}\\
y_{k/c}\\
z_{k/c}
\end{bmatrix}\right)
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}x_{i/k}\\
y_{i/k}\\
z_{i/k}
\end{bmatrix}=\begin{bmatrix}r_{11}\left(x_{i/c}-x_{k/c}\right)+r_{21}\left(y_{i/c}-y_{k/c}\right)+r_{31}\left(z_{i/c}-z_{k/c}\right)\\
r_{12}\left(x_{i/c}-x_{k/c}\right)+r_{22}\left(y_{i/c}-y_{k/c}\right)+r_{32}\left(z_{i/c}-z_{k/c}\right)\\
r_{13}\left(x_{i/c}-x_{k/c}\right)+r_{23}\left(y_{i/c}-y_{k/c}\right)+r_{33}\left(z_{i/c}-z_{k/c}\right)
\end{bmatrix}
\]

\end_inset

using the pinhole model
\begin_inset Formula 
\[
A\triangleq\begin{bmatrix}f_{x} & 0 & c_{x}\\
0 & f_{y} & c_{y}\\
0 & 0 & 1
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
m_{i/k}\triangleq\begin{bmatrix}\frac{x_{i/k}}{z_{i/k}}\\
\frac{y_{i/k}}{z_{i/k}}\\
1
\end{bmatrix}
\]

\end_inset

 
\begin_inset Formula 
\[
\begin{bmatrix}u_{i/k}\\
v_{i/k}\\
1
\end{bmatrix}=Am_{i/k}
\]

\end_inset


\begin_inset Formula 
\[
m_{i/k}=\begin{bmatrix}\frac{r_{11}\left(x_{i/c}-x_{k/c}\right)+r_{21}\left(y_{i/c}-y_{k/c}\right)+r_{31}\left(z_{i/c}-z_{k/c}\right)}{r_{13}\left(x_{i/c}-x_{k/c}\right)+r_{23}\left(y_{i/c}-y_{k/c}\right)+r_{33}\left(z_{i/c}-z_{k/c}\right)}\\
\frac{r_{12}\left(x_{i/c}-x_{k/c}\right)+r_{22}\left(y_{i/c}-y_{k/c}\right)+r_{32}\left(z_{i/c}-z_{k/c}\right)}{r_{13}\left(x_{i/c}-x_{k/c}\right)+r_{23}\left(y_{i/c}-y_{k/c}\right)+r_{33}\left(z_{i/c}-z_{k/c}\right)}\\
1
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}u_{i/k}\\
v_{i/k}\\
1
\end{bmatrix}=\begin{bmatrix}f_{x} & 0 & c_{x}\\
0 & f_{y} & c_{y}\\
0 & 0 & 1
\end{bmatrix}\begin{bmatrix}\frac{r_{11}\left(x_{i/c}-x_{k/c}\right)+r_{21}\left(y_{i/c}-y_{k/c}\right)+r_{31}\left(z_{i/c}-z_{k/c}\right)}{r_{13}\left(x_{i/c}-x_{k/c}\right)+r_{23}\left(y_{i/c}-y_{k/c}\right)+r_{33}\left(z_{i/c}-z_{k/c}\right)}\\
\frac{r_{12}\left(x_{i/c}-x_{k/c}\right)+r_{22}\left(y_{i/c}-y_{k/c}\right)+r_{32}\left(z_{i/c}-z_{k/c}\right)}{r_{13}\left(x_{i/c}-x_{k/c}\right)+r_{23}\left(y_{i/c}-y_{k/c}\right)+r_{33}\left(z_{i/c}-z_{k/c}\right)}\\
1
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}u_{i/k}\\
v_{i/k}\\
1
\end{bmatrix}=\begin{bmatrix}f_{x}\frac{r_{11}\left(x_{i/c}-x_{k/c}\right)+r_{21}\left(y_{i/c}-y_{k/c}\right)+r_{31}\left(z_{i/c}-z_{k/c}\right)}{r_{13}\left(x_{i/c}-x_{k/c}\right)+r_{23}\left(y_{i/c}-y_{k/c}\right)+r_{33}\left(z_{i/c}-z_{k/c}\right)}+c_{x}\\
f_{y}\frac{r_{12}\left(x_{i/c}-x_{k/c}\right)+r_{22}\left(y_{i/c}-y_{k/c}\right)+r_{32}\left(z_{i/c}-z_{k/c}\right)}{r_{13}\left(x_{i/c}-x_{k/c}\right)+r_{23}\left(y_{i/c}-y_{k/c}\right)+r_{33}\left(z_{i/c}-z_{k/c}\right)}+c_{y}\\
1
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}u_{i/k}\\
v_{i/k}
\end{bmatrix}=\begin{bmatrix}f_{x}\frac{r_{11}\left(x_{i/c}-x_{k/c}\right)+r_{21}\left(y_{i/c}-y_{k/c}\right)+r_{31}\left(z_{i/c}-z_{k/c}\right)}{r_{13}\left(x_{i/c}-x_{k/c}\right)+r_{23}\left(y_{i/c}-y_{k/c}\right)+r_{33}\left(z_{i/c}-z_{k/c}\right)}+c_{x}\\
f_{y}\frac{r_{12}\left(x_{i/c}-x_{k/c}\right)+r_{22}\left(y_{i/c}-y_{k/c}\right)+r_{32}\left(z_{i/c}-z_{k/c}\right)}{r_{13}\left(x_{i/c}-x_{k/c}\right)+r_{23}\left(y_{i/c}-y_{k/c}\right)+r_{33}\left(z_{i/c}-z_{k/c}\right)}+c_{y}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
m_{i/c}\triangleq\begin{bmatrix}\frac{x_{i/c}}{z_{i/c}}\\
\frac{y_{i/c}}{z_{i/c}}\\
1
\end{bmatrix}
\]

\end_inset

 
\begin_inset Formula 
\[
\begin{bmatrix}u_{i/c}\\
v_{i/c}\\
1
\end{bmatrix}=\begin{bmatrix}f_{x} & 0 & c_{x}\\
0 & f_{y} & c_{y}\\
0 & 0 & 1
\end{bmatrix}\begin{bmatrix}\frac{x_{i/c}}{z_{i/c}}\\
\frac{y_{i/c}}{z_{i/c}}\\
1
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}u_{i/c}\\
v_{i/c}\\
1
\end{bmatrix}=\begin{bmatrix}f_{x}\frac{x_{i/c}}{z_{i/c}}+c_{x}\\
f_{y}\frac{y_{i/c}}{z_{i/c}}+c_{y}\\
1
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}\frac{x_{i/c}}{z_{i/c}}\\
\frac{y_{i/c}}{z_{i/c}}
\end{bmatrix}=\begin{bmatrix}\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}\\
\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}x_{i/c}\\
y_{i/c}
\end{bmatrix}=\begin{bmatrix}\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}\\
\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}u_{i/k}\\
v_{i/k}
\end{bmatrix}=\begin{bmatrix}f_{x}\frac{r_{11}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)+r_{21}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)+r_{31}\left(z_{i/c}-z_{k/c}\right)}{r_{13}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)+r_{23}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)+r_{33}\left(z_{i/c}-z_{k/c}\right)}+c_{x}\\
f_{y}\frac{r_{12}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)+r_{22}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)+r_{32}\left(z_{i/c}-z_{k/c}\right)}{r_{13}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)+r_{23}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)+r_{33}\left(z_{i/c}-z_{k/c}\right)}+c_{y}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
X_{i}=\begin{bmatrix}u_{i/c}\\
v_{i/c}\\
z_{i/c}\\
q_{w}\\
q_{x}\\
q_{y}\\
q_{z}\\
x_{k/c}\\
y_{k/c}\\
z_{k/c}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
J_{i}=\begin{bmatrix}\frac{\partial u_{i/k}}{\partial u_{i/c}} & \frac{\partial u_{i/k}}{\partial v_{i/c}} & \frac{\partial u_{i/k}}{\partial z_{i/c}} & \frac{\partial u_{i/k}}{\partial q_{w}} & \frac{\partial u_{i/k}}{\partial q_{x}} & \frac{\partial u_{i/k}}{\partial q_{y}} & \frac{\partial u_{i/k}}{\partial q_{z}} & \frac{\partial u_{i/k}}{\partial x_{k/c}} & \frac{\partial u_{i/k}}{\partial y_{k/c}} & \frac{\partial u_{i/k}}{\partial z_{k/c}}\\
\frac{\partial v_{i/k}}{\partial u_{i/c}} & \frac{\partial v_{i/k}}{\partial v_{i/c}} & \frac{\partial v_{i/k}}{\partial z_{i/c}} & \frac{\partial v_{i/k}}{\partial q_{w}} & \frac{\partial v_{i/k}}{\partial q_{x}} & \frac{\partial v_{i/k}}{\partial q_{y}} & \frac{\partial v_{i/k}}{\partial q_{z}} & \frac{\partial v_{i/k}}{\partial x_{k/c}} & \frac{\partial v_{i/k}}{\partial y_{k/c}} & \frac{\partial v_{i/k}}{\partial z_{k/c}}
\end{bmatrix}
\]

\end_inset


\end_layout

\begin_layout Standard
partials for rotation elements
\begin_inset Formula 
\[
\frac{\partial r_{11}}{\partial q}=\begin{bmatrix}0 & 0 & -4q_{y} & -4q_{z}\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial r_{12}}{\partial q}=\begin{bmatrix}-2q_{z} & 2q_{y} & 2q_{x} & -2q_{w}\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial r_{13}}{\partial q}=\begin{bmatrix}2q_{y} & 2q_{z} & 2q_{w} & 2q_{x}\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial r_{21}}{\partial q}=\begin{bmatrix}2q_{z} & 2q_{y} & 2q_{x} & 2q_{w}\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial r_{22}}{\partial q}=\begin{bmatrix}0 & -4q_{x} & 0 & -4q_{z}\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial r_{23}}{\partial q}=\begin{bmatrix}-2q_{x} & -2q_{w} & 2q_{z} & 2q_{y}\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial r_{31}}{\partial q}=\begin{bmatrix}2\left(q_{z}-q_{y}\right) & 0 & -2q_{w} & 2q_{w}\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial r_{32}}{\partial q}=\begin{bmatrix}2q_{x} & 2q_{w} & 2q_{z} & 2q_{y}\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial r_{33}}{\partial q}=\begin{bmatrix}0 & -4q_{x} & -4q_{y} & 0\end{bmatrix}
\]

\end_inset


\end_layout

\begin_layout Standard
partials of uik
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial u_{i/k}}{\partial u_{i/c}}=f_{x}\frac{\left(r_{13}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)+r_{23}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)+r_{33}\left(z_{i/c}-z_{k/c}\right)\right)r_{11}\frac{z_{i/c}}{f_{x}}-\left(r_{11}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)+r_{21}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)+r_{31}\left(z_{i/c}-z_{k/c}\right)\right)r_{13}\frac{z_{i/c}}{f_{x}}}{\left(r_{13}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)+r_{23}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)+r_{33}\left(z_{i/c}-z_{k/c}\right)\right)^{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial u_{i/k}}{\partial v_{i/c}}=f_{x}\frac{\left(r_{13}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)+r_{23}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)+r_{33}\left(z_{i/c}-z_{k/c}\right)\right)r_{21}\frac{z_{i/c}}{f_{y}}-\left(r_{11}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)+r_{21}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)+r_{31}\left(z_{i/c}-z_{k/c}\right)\right)r_{23}\frac{z_{i/c}}{f_{y}}}{\left(r_{13}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)+r_{23}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)+r_{33}\left(z_{i/c}-z_{k/c}\right)\right)^{2}}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial u_{i/k}}{\partial z_{i/c}}=f_{x}\frac{\left(r_{13}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)+r_{23}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)+r_{33}\left(z_{i/c}-z_{k/c}\right)\right)\left(r_{11}\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}+r_{21}\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}+r_{31}\right)-\left(r_{11}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)+r_{21}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)+r_{31}\left(z_{i/c}-z_{k/c}\right)\right)\left(r_{13}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}\right)+r_{23}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}\right)+r_{33}\right)}{\left(r_{13}\left(\frac{\left(u_{i/c}-c_{x}\right)}{f_{x}}z_{i/c}-x_{k/c}\right)+r_{23}\left(\frac{\left(v_{i/c}-c_{y}\right)}{f_{y}}z_{i/c}-y_{k/c}\right)+r_{33}\left(z_{i/c}-z_{k/c}\right)\right)^{2}}
\]

\end_inset


\begin_inset Formula 
\[
