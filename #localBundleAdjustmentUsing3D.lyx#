\begin_layout Standard
stacking the partials across all features
\begin_inset Formula 
\[
X_{i}=\begin{bmatrix}q_{w}\\
q_{x}\\
q_{y}\\
q_{z}\\
x_{k/c}\\
y_{k/c}\\
z_{k/c}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
X=\begin{bmatrix}z_{1/c}\\
\vdots\\
z_{n/c}\\
q_{w}\\
q_{x}\\
q_{y}\\
q_{z}\\
x_{k/c}\\
y_{k/c}\\
z_{k/c}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
J=\begin{bmatrix}\frac{\partial u_{1/k}}{\partial z_{1/c}} & 0 & \dots & 0 & \frac{\partial u_{1/k}}{\partial q_{w}} & \frac{\partial u_{1/k}}{\partial q_{x}} & \frac{\partial u_{1/k}}{\partial q_{y}} & \frac{\partial u_{1/k}}{\partial q_{z}} & \frac{\partial u_{1/k}}{\partial x_{k/c}} & \frac{\partial u_{1/k}}{\partial y_{k/c}} & \frac{\partial u_{1/k}}{\partial z_{k/c}}\\
\frac{\partial v_{1/k}}{\partial z_{1/c}} & 0 & \dots & 0 & \frac{\partial v_{1/k}}{\partial q_{w}} & \frac{\partial v_{1/k}}{\partial q_{x}} & \frac{\partial v_{1/k}}{\partial q_{y}} & \frac{\partial v_{1/k}}{\partial q_{z}} & \frac{\partial v_{1/k}}{\partial x_{k/c}} & \frac{\partial v_{1/k}}{\partial y_{k/c}} & \frac{\partial v_{1/k}}{\partial z_{k/c}}\\
0 & \frac{\partial u_{2/k}}{\partial z_{2/c}} & \ddots & 0 & \frac{\partial u_{2/k}}{\partial q_{w}} & \frac{\partial u_{2/k}}{\partial q_{x}} & \frac{\partial u_{2/k}}{\partial q_{y}} & \frac{\partial u_{2/k}}{\partial q_{z}} & \frac{\partial u_{2/k}}{\partial x_{k/c}} & \frac{\partial u_{2/k}}{\partial y_{k/c}} & \frac{\partial u_{2/k}}{\partial z_{k/c}}\\
0 & \frac{\partial v_{2/k}}{\partial z_{2/c}} & \ddots & 0 & \frac{\partial v_{2/k}}{\partial q_{w}} & \frac{\partial v_{2/k}}{\partial q_{x}} & \frac{\partial v_{2/k}}{\partial q_{y}} & \frac{\partial v_{2/k}}{\partial q_{z}} & \frac{\partial v_{2/k}}{\partial x_{k/c}} & \frac{\partial v_{2/k}}{\partial y_{k/c}} & \frac{\partial v_{2/k}}{\partial z_{k/c}}\\
\vdots & \ddots & \ddots & \vdots & \vdots & \vdots & \vdots & \vdots & \vdots & \vdots & \vdots\\
0 & 0 & \dots & \frac{\partial u_{n/k}}{\partial z_{n/c}} & \frac{\partial u_{n/k}}{\partial q_{w}} & \frac{\partial u_{n/k}}{\partial q_{x}} & \frac{\partial u_{n/k}}{\partial q_{y}} & \frac{\partial u_{n/k}}{\partial q_{z}} & \frac{\partial u_{n/k}}{\partial x_{k/c}} & \frac{\partial u_{n/k}}{\partial y_{k/c}} & \frac{\partial u_{n/k}}{\partial z_{k/c}}\\
0 & 0 & \dots & \frac{\partial v_{n/k}}{\partial z_{n/c}} & \frac{\partial v_{n/k}}{\partial q_{w}} & \frac{\partial v_{n/k}}{\partial q_{x}} & \frac{\partial v_{n/k}}{\partial q_{y}} & \frac{\partial v_{n/k}}{\partial q_{z}} & \frac{\partial v_{n/k}}{\partial x_{k/c}} & \frac{\partial v_{n/k}}{\partial y_{k/c}} & \frac{\partial v_{n/k}}{\partial z_{k/c}}
\end{bmatrix}
\]

\end_inset


