\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial r_{31}}{\partial q}=\begin{bmatrix}2\left(q_{z}-q_{y}\right) & 0 & -2q_{w} & 2q_{w}\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial r_{32}}{\partial q}=\begin{bmatrix}2q_{x} & 2q_{w} & 2q_{z} & 2q_{y}\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial r_{33}}{\partial q}=\begin{bmatrix}0 & -4q_{x} & -4q_{y} & 0\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial z_{i/c}}{\partial q_{w}}=z_{k/c}+r_{31}x_{i/k}+r_{32}y_{i/k}+r_{33}z_{i/k}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
