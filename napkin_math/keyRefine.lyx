\begin_layout Standard
\begin_inset Formula 
\[
\underline{p}_{i/c}\left(t\right)=\underline{p}_{k/c}\left(t\right)+R_{k/c}\left(t\right)\underline{p}_{i/k}
\]

\end_inset


\begin_inset Formula 
\[
R_{k/c}\left(t\right)=\begin{bmatrix}r_{11} & r_{12} & r_{13}\\
r_{21} & r_{22} & r_{23}\\
r_{31} & r_{32} & r_{33}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
q_{k/c}\left(t\right)=\begin{bmatrix}q_{w}\left(t\right)\\
q_{x}\left(t\right)\\
q_{y}\left(t\right)\\
q_{z}\left(t\right)
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\underline{p}_{k/c}\left(t\right)=\begin{bmatrix}x_{k/c}\left(t\right)\\
y_{k/c}\left(t\right)\\
z_{k/c}\left(t\right)
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
r_{11}=1-2\left(q_{y}^{2}+q_{z}^{2}\right)
\]

\end_inset


\begin_inset Formula 
\[
r_{12}=2\left(q_{x}q_{y}-q_{z}q_{w}\right)
\]

\end_inset


\begin_inset Formula 
\[
r_{13}=2\left(q_{x}q_{z}+q_{y}q_{w}\right)
\]

\end_inset


\begin_inset Formula 
\[
r_{21}=2\left(q_{x}q_{y}+q_{z}q_{w}\right)
\]

\end_inset


\begin_inset Formula 
\[
r_{22}=1-2\left(q_{x}^{2}+q_{z}^{2}\right)
\]

\end_inset


\begin_inset Formula 
\[
r_{23}=2\left(q_{y}q_{z}-q_{x}q_{w}\right)
\]

\end_inset


\begin_inset Formula 
\[
r_{31}=2\left(q_{w}q_{z}-q_{y}q_{w}\right)
\]

\end_inset


\begin_inset Formula 
\[
r_{32}=2\left(q_{y}q_{z}+q_{x}q_{w}\right)
\]

\end_inset


\begin_inset Formula 
\[
r_{33}=1-2\left(q_{x}^{2}+q_{y}^{2}\right)
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}x_{i/c}\\
y_{i/c}\\
z_{i/c}
\end{bmatrix}=\begin{bmatrix}x_{k/c}\\
y_{k/c}\\
z_{k/c}
\end{bmatrix}+\begin{bmatrix}r_{11} & r_{12} & r_{13}\\
r_{21} & r_{22} & r_{23}\\
r_{31} & r_{32} & r_{33}
\end{bmatrix}\begin{bmatrix}x_{i/k}\\
y_{i/k}\\
z_{i/k}
\end{bmatrix}
\]

\end_inset


\end_layout

\begin_layout Standard
dropping the time for notational briefness
\begin_inset Formula 
\[
