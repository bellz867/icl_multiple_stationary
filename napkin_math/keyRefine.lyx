y_{i/c}\\
z_{i/c}
\end{bmatrix}=\begin{bmatrix}x_{k/c}+r_{11}x_{i/k}+r_{12}y_{i/k}+r_{13}z_{i/k}\\
y_{k/c}+r_{21}x_{i/k}+r_{22}y_{i/k}+r_{23}z_{i/k}\\
z_{k/c}+r_{31}x_{i/k}+r_{32}y_{i/k}+r_{33}z_{i/k}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}u_{i/c}\\
v_{i/c}\\
1
\end{bmatrix}=\begin{bmatrix}f_{x} & 0 & c_{x}\\
0 & f_{y} & c_{y}\\
0 & 0 & 1
\end{bmatrix}\begin{bmatrix}\frac{x_{i/c}}{z_{i/c}}\\
\frac{y_{i/c}}{z_{i/c}}\\
1
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}u_{i/c}\\
v_{i/c}
\end{bmatrix}=\begin{bmatrix}f_{x}\frac{x_{i/c}}{z_{i/c}}+c_{x}\\
f_{y}\frac{y_{i/c}}{z_{i/c}}+c_{y}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\begin{bmatrix}u_{i/c}\\
v_{i/c}
\end{bmatrix}=\begin{bmatrix}f_{x}\frac{x_{k/c}+r_{11}x_{i/k}+r_{12}y_{i/k}+r_{13}z_{i/k}}{z_{k/c}+r_{31}x_{i/k}+r_{32}y_{i/k}+r_{33}z_{i/k}}+c_{x}\\
f_{y}\frac{y_{k/c}+r_{21}x_{i/k}+r_{22}y_{i/k}+r_{23}z_{i/k}}{z_{k/c}+r_{31}x_{i/k}+r_{32}y_{i/k}+r_{33}z_{i/k}}+c_{y}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
X_{i}=\begin{bmatrix}u_{i/c}\\
v_{i/c}\\
z_{i/c}\\
